'edges': 1, 'will': 1, 'hexagons,': 1, 'to': 1, 'sinking': 1, 'mind': 1, 'have': 2, 'views': 1,
'pretty': 1, 'like': 1, 'with': 1, 'years': 1, 'said': 1, 'the': 2, 'remaining': 1, 'but': 2,
'"my': 1, 'sheet': 1, 'been': 1, 'now': 1, 'alas,': 1, 'luminous': 1, 'opened': 1, 'freely': 1,
'of': 5, 'without': 1, 'rising': 1, 'a': 3, 'then': 1, 'which': 1, 'hard': 1, 'i': 1, 'vast': 1,
'correct': 1, 'squares,': 1, '-': 2, 'straight': 1, 'above': 1, 'about,': 1, 'you': 1, 'very': 1,
'imagine': 1, 'on': 2, 'places,': 1, 'below': 1, 'triangles,': 1, 'lines,': 1, 'or': 2, 'paper': 1,
'it,': 1, 'universe":': 1, 'higher': 1, 'country': 1, 'and': 4, 'their': 1, 'instead': 1, 'in': 2,
'move': 1, 'pentagons,': 1, 'fixed': 1, 'things.': 1, 'much': 1, 'few': 1, 'should': 1, 'shadows': 1,
'has': 1, 'notion': 1, 'only': 1}
