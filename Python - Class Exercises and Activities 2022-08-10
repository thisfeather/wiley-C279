# add key-value pair
storage["freezer"] = ["ice cubes", "ice cream", "pepperoni pizza"]

# sort cupboard
storage["cupboard"].sort()

# add cream
storage["fridge"].append("cream")

# remove sugar
storage["cupboard"].remove("sugar")

# subtract $25
storage["emergency jar"] -= 25 

print("Final outcome of storage:")
storage
